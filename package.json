  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FellouAI/eko.git"
  },
  "devDependencies": {
    "inspector": "^0.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@eko-ai/eko": "link:",
    "@langfuse/client": "^4.2.0",
    "@langfuse/tracing": "^4.2.0"
  }
}
