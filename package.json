{
  "name": "@eko-ai/eko",
  "version": "3.0.9-alpha.1",
  "description": "Empowering language to transform human words into action.",
  "workspaces": [
    "packages/eko-core",
    "packages/eko-extension",
    "packages/eko-nodejs",
    "packages/eko-web",
    "packages/eko-debugger"
  ],
  "scripts": {
    "build": "pnpm -r --sequential build",
    "build-eko": "pnpm --filter @eko-ai/eko build",
